{
    "name": "markdown-preview-enhanced",
    "displayName": "markdown-preview-enhanced",
    "description": "Markdown Preview Enhanced ported to vscode",
    "version": "0.0.1",
    "publisher": "shd101wyy",
    "repository": "https://github.com/shd101wyy/vscode-markdown-preview-enhanced",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:markdown"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "markdown-preview-enhanced.toggle",
                "title": "Markdown Preview Enhanced: Toggle Preview"
            }
        ],
        "keybindings": [
            {
                "command": "markdown-preview-enhanced.toggle",
                "key": "shift+ctrl+m",
                "when": "editorLangId == markdown"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
